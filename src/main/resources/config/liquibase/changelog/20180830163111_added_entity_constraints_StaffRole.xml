<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity StaffRole.
    -->
    <changeSet id="20180830163111-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="staff_id"
                                 baseTableName="staff_role"
                                 constraintName="fk_staff_role_staff_id"
                                 referencedColumnNames="id"
                                 referencedTableName="staff"/>

        <addForeignKeyConstraint baseColumnNames="role_id"
                                 baseTableName="staff_role"
                                 constraintName="fk_staff_role_role_id"
                                 referencedColumnNames="id"
                                 referencedTableName="role"/>

    </changeSet>
</databaseChangeLog>
